{
	"name": "@xumi/rhooks",
	"version": "1.1.9",
	"description": "一个基于 React Hooks 的工具库",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx",
		"prepublishOnly": "npm run build",
		"demo": "vite"
	},
	"keywords": [
		"react",
		"hooks",
		"react-hooks",
		"typescript",
		"frontend",
		"ui",
		"components"
	],
	"author": "liuyuanquan",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/liuyuanquan/rhooks.git"
	},
	"bugs": {
		"url": "https://github.com/liuyuanquan/rhooks/issues"
	},
	"homepage": "https://github.com/liuyuanquan/rhooks#readme",
	"peerDependencies": {
		"react": ">=16.8.0"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.0",
		"eslint": "^8.0.0",
		"react": "^18.3.1",
		"tsup": "^8.0.0",
		"typescript": "^5.0.0",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"react-dom": "^18.3.1"
	}
}
